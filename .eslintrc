{
  "extends": ["eslint-config-airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "ecmaVersion": 6,
  "ecmaFeatures": {
    "jsx": true,
    "arrowFunctions": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "babel"
  ],
  "rules": {
    "constructor-super": 2,
    "comma-dangle": 0,
    "eqeqeq": [2, "allow-null"],
    "id-length": 0,
    "indent": [2, 2, { "SwitchCase": 1, "VariableDeclarator": 1 }],
    "no-eq-null": 0,
    "one-var": [2, { "initialized": "never" }],
    "prefer-const": 0,
    "no-param-reassign": 0,
    "no-this-before-super": 2,
    "babel/object-shorthand": 2,
    "react/prefer-stateless-function": 0,
    "react/jsx-boolean-value": 2,
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-indent": 2,
    "react/jsx-indent-props": 2,
    "react/jsx-filename-extension": 0,
    "react/sort-comp": 0,
    "react/prop-types": 0,
    "react/jsx-no-bind": 2
  }
}